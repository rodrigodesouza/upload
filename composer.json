{
    "name": "bredi/upload",
    "description": "upload de imagens e arquivos",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "rodrigo souza",
            "email": "rd7.rodrigo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bredi\\Upload\\": "src/"
        },
        "files": ["app/Helpers/Helper.php"]
    },
    "require": {
        "php": ">=5.6",
        "illuminate/support": "4.2.*|5.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bredi\\Upload\\UploadServiceProvider",
                "Intervention\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "Upload": "Bredi\\Upload\\Facade",
                "Image": "Intervention\\Image\\Facades\\Image"
            }
        }
    }
}
